--obtiene todos los valores de la tabla "users" con email distinto a sara@gmail.com
SELECT * FROM users WHERE NOT email =  "sara@gmail.com";

--obtiene todos los valores de la tabla "users" con email distinto a sara@gmail.com y edad igual a 18
SELECT * FROM users WHERE NOT email =  "sara@gmail.com" AND age = 18;

--obtiene todos los valores de la tabla "users" con email distinto a sara@gmail.com O edad igual a 18
SELECT * FROM users WHERE NOT email =  "sara@gmail.com" OR age = 18;